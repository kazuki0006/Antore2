# ãƒ‡ãƒ¼ã‚¿ã‚’æ ¼ç´ã™ã‚‹ãŸã‚ã®ãƒªã‚¹ãƒˆ
cat1_data = []

# ã‚¹ã‚³ã‚¢è¨ˆç®—é–¢æ•°
def calculate_score(duration):#ğŸ‘ˆç›´ç·šçš„ã§ãªãæ›²ç·šçš„ãªå‹¾é…
    if duration <= 5:
        return 100
    elif duration <= 10:
        return 90
    elif duration <= 15:
        return 80
    elif duration <= 20:
        return 70
    elif duration <= 25:
        return 65
    elif duration <= 30:
        return 60
    elif duration <= 40:
        return 55
    elif duration <= 50:
        return 50
    elif duration <= 60:
        return 30
    else:
        return 10

for origin in addresses:
    nav_request = 'language=ja&origin={}&destination={}&key={}'.format(origin, destinations, api_key)#ğŸ‘ˆé›»è»Šã§ã®å‡ºåŠ›ã¯ä¸€æ„ã«ãªã‚‰ãšDFåŒ–ãŒé›£ã—ã„â‡’å„ªå…ˆåº¦ä¸‹ã’ã¦å¾Œã§ä¿®æ­£ã‚’æ¤œè¨
    nav_request = urllib.parse.quote_plus(nav_request, safe='=&')
    request = endpoint + nav_request

    # Google Maps Platform Directions APIã‚’å®Ÿè¡Œ
    response = urllib.request.urlopen(request).read()

    # çµæœ(JSON)ã‚’å–å¾—
    directions = json.loads(response)

    # æ‰€è¦æ™‚é–“ã‚’å–å¾—
    for key in directions['routes']:
        for key2 in key['legs']:
            distance = key2['distance']['text']
            duration = key2['duration']['text']

            # è·é›¢ã®æ–‡å­—åˆ—ã‹ã‚‰"km"ã‚’å–ã‚Šé™¤ã„ã¦æ•°å€¤ã«å¤‰æ›
            distance = float(distance.replace(' km', '').replace(',', ''))

            # æ™‚é–“ã®æ–‡å­—åˆ—ã‹ã‚‰"åˆ†"ã‚’å–ã‚Šé™¤ã„ã¦æ•°å€¤ã«å¤‰æ›
            duration = int(duration.replace('åˆ†', ''))#ğŸ‘ˆ1æ™‚é–“ã‚’è¶…ãˆã‚‹ã¨"xxæ™‚é–“"ã¨è¡¨ç¤ºã•ã‚Œã‚‹ãŸã‚ã‚¨ãƒ©ãƒ¼ã«ãªã‚‹ã€‚è¦ä¿®æ­£

            # ã‚¹ã‚³ã‚¢ã‚’è¨ˆç®—
            score = calculate_score(duration)

            cat1_data.append([origin, distance, duration, score])

# ãƒ‡ãƒ¼ã‚¿ã‚’DataFrameã«å¤‰æ›
df1 = pd.DataFrame(cat1_data, columns=['å€™è£œç‰©ä»¶ä½æ‰€', 'ç›®çš„åœ°ã¾ã§ã®è·é›¢(km)', 'åˆ°ç€ã¾ã§ã«ã‹ã‹ã‚‹æ™‚é–“(åˆ†)', "ã‚¹ã‚³ã‚¢"])






###"å­è‚²ã¦ã®ã—ã‚„ã™ã•"ã®å‡¦ç†###
# ã‚¯ãƒ©ã‚¤ã‚¢ãƒ³ãƒˆã®ç”Ÿæˆ
gmaps = googlemaps.Client(key=api_key)

# æ–½è¨­å
place_name = "ä¿è‚²åœ’"#ğŸ‘ˆæ–½è¨­åã‚’å¤‰ãˆãŸãƒ‘ã‚¿ãƒ¼ãƒ³ã‚’ç”¨æ„

# ãƒ‡ãƒ¼ã‚¿ã‚’æ ¼ç´ã™ã‚‹ãŸã‚ã®ãƒªã‚¹ãƒˆ
data_list = []

# å„ä½æ‰€ã«å¯¾ã—ã¦å‡¦ç†ã‚’ç¹°ã‚Šè¿”ã™
for address in addresses:
    # ä½æ‰€ã‹ã‚‰ç·¯åº¦çµŒåº¦ã‚’å–å¾—
    geocode_result = gmaps.geocode(address)
    location = geocode_result[0]['geometry']['location']

    # æ¤œç´¢ç¯„å›²ã®æŒ‡å®š (åŠå¾„1.2 km)
    radius = 500  # ğŸ‘ˆ1200mä»¥å†…ã ã¨ã‚«ã‚¦ãƒ³ãƒˆã®æœ€å¤§å€¤20ã‚’ã™ãã«è¶…ãˆã‚‹+"å­¦æ ¡"ã¨ãªã£ã¦ã„ã‚‹ä¿è‚²åœ’ã‚’ã‚«ã‚¦ãƒ³ãƒˆã—ãªã„ã€å‡¦ç†æ™‚é–“ã‚‚åŠ å‘³ã—ã¦è¦ç›¸è«‡
                  # ğŸ‘ˆå‡ºåŠ›ã•ã‚Œã‚‹æ–½è¨­æ•°ãŒ500mä»¥å†…ã‹ã©ã†ã‹æ€ªã—ã„

    # æ¤œç´¢çµæœã‚’å–å¾—
    places_result = gmaps.places_nearby(location=location, radius=radius, keyword=place_name, language='ja')

    # 500måŠå¾„å†…ã®æ–½è¨­æ•°ã‚’æ ¼ç´ã™ã‚‹å¤‰æ•°
    num_places_within_radius = 0

    # æ¤œç´¢çµæœã‚’ãƒ«ãƒ¼ãƒ—ã—ã¦500måŠå¾„å†…ã®æ–½è¨­æ•°ã‚’ã‚«ã‚¦ãƒ³ãƒˆ
    for place in places_result['results']:
        # æ–½è¨­ã¾ã§ã®æ‰€è¦æ™‚é–“(å¾’æ­©)ã‚’å–å¾—
        distance_matrix_result = gmaps.distance_matrix(location, place['geometry']['location'], mode='walking', language='ja', units='metric')
        distance = distance_matrix_result['rows'][0]['elements'][0]['distance']['value']

        # åŠå¾„500mä»¥å†…ã®å ´åˆã«ã‚«ã‚¦ãƒ³ãƒˆ
        if distance <= radius:
            num_places_within_radius += 1
          
    # æ–½è¨­æ•°ã‚’ã‚¹ã‚³ã‚¢ãƒªãƒ³ã‚°ğŸ‘ˆã‚¹ã‚³ã‚¢ãƒªãƒ³ã‚°ã®æ–¹æ³•ã¯è¦ç›¸è«‡
    if num_places_within_radius >= 5:
        cnt_score = 100
    elif 4 <= num_places_within_radius < 5:
        cnt_score = 90
    elif 3 <= num_places_within_radius < 4:
        cnt_score = 75
    elif 2 <= num_places_within_radius < 3:
        cnt_score = 55
    elif 1 <= num_places_within_radius < 2:
        cnt_score = 50
    else:
        cnt_score = 10
        
    # æœ€ã‚‚è¿‘ã„æ–½è¨­ã¾ã§ã®æ‰€è¦æ™‚é–“ã‚’æ•´æ•°ã«å¤‰æ›ã—ã¦ã‚¹ã‚³ã‚¢ãƒªãƒ³ã‚°
    nearest_distance = float('inf')
    nearest_duration = None
    nearest_place_name = None
    
    for place in places_result['results']:
        # æ–½è¨­ã¾ã§ã®æ‰€è¦æ™‚é–“ã‚’å–å¾—
        distance_matrix_result = gmaps.distance_matrix(location, place['geometry']['location'], mode='walking', language='ja', units='metric')
        duration_text = distance_matrix_result['rows'][0]['elements'][0]['duration']['text']

        # æœ€ã‚‚è¿‘ã„æ–½è¨­ã‚’æ›´æ–°
        if 'distance' in distance_matrix_result['rows'][0]['elements'][0]:
            distance = distance_matrix_result['rows'][0]['elements'][0]['distance']['value']
            if distance < nearest_distance:
                nearest_distance = distance
                nearest_duration = duration_text
                nearest_place_name = place['name']
    
    #ğŸ‘ˆè¦ç›¸è«‡
    duration_int = int(nearest_duration.replace("åˆ†", ""))
    # if duration_int <= 2:
    #     dur_score = 100
    # elif 2 < duration_int <= 5:
    #     dur_score = 80
    # elif 5 < duration_int <= 10:
    #     dur_score = 60
    # else:
    #     dur_score = 10
    dur_score = 100 - (duration_int - 1)*5
                
    #ã‚«ãƒ†ã‚´ãƒªã®ã‚¹ã‚³ã‚¢ãƒªãƒ³ã‚°
    #ğŸ‘ˆã‚¹ã‚³ã‚¢ãƒªãƒ³ã‚°ã®æ–¹æ³•ã¯è¦ç›¸è«‡
    category_score = (cnt_score + dur_score) / 2
    #éƒ¨åˆ†çš„ã«ä½ã„ç‚¹æ•°ãŒã‚ã‚‹å ´åˆã«ã€è¶³ã—ç®—ã¨ã™ã‚‹ã‹æ›ã‘ç®—ã¨ã™ã‚‹ã‹
    #è¶³ã—ç®—â‡’åŠ é‡å¹³å‡(ã©ã¡ã‚‰ã‹ã«æ¿ƒæ·¡ã¤ã‘ã‚‹ã¹ãã‹)
    #èª¿ã¹ã‚‹æ®µéšã§ã¯æ–½è¨­æ•°ã€ä½ã‚“ã§ã‹ã‚‰ã¯è·é›¢ã‚’é‡è¦–ã™ã‚‹â‡’ã©ã¡ã‚‰ã‹ã ã‚ãªã‚‰ã ã‚
            
    # å‡¦ç†çµæœã‚’è¾æ›¸ã«æ ¼ç´
    cat3_data = {'å€™è£œç‰©ä»¶ä½æ‰€': address,
            'ã‚¨ãƒªã‚¢å†…ã®æ–½è¨­æ•°': num_places_within_radius,
            'æœ€ã‚‚è¿‘ã„æ–½è¨­å': nearest_place_name,
            'æœ€ã‚‚è¿‘ã„æ–½è¨­ã¾ã§ã®æ‰€è¦æ™‚é–“(åˆ†)': nearest_duration,
            "ä¿è‚²åœ’ã®ã‚¹ã‚³ã‚¢":category_score,
            "æ–½è¨­æ•°ã‚¹ã‚³ã‚¢":cnt_score,
            "æ‰€è¦æ™‚é–“ã‚¹ã‚³ã‚¢":dur_score}

    # ãƒ‡ãƒ¼ã‚¿ã‚’ãƒªã‚¹ãƒˆã«è¿½åŠ 
    data_list.append(cat3_data)

# ãƒªã‚¹ãƒˆã‹ã‚‰ãƒ‡ãƒ¼ã‚¿ãƒ•ãƒ¬ãƒ¼ãƒ ã‚’ä½œæˆ
df3 = pd.DataFrame(data_list)





